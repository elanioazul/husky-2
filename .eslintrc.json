{
    "env": {
        "browser": true,
        "es2017": true,
        "es6": true,
        "node": true
    },
    "extends": [
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto"
        }
      ]
    }
}
